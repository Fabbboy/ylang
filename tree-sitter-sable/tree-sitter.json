{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sable",
      "camelcase": "Sable",
      "title": "Sable",
      "scope": "source.sable",
      "file-types": [
        "sable"
      ],
      "injection-regex": "^sable$",
      "class-name": "TreeSitterSable"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache-2.0",
    "description": "Sable is a compiled language inspired by C, Rust and Swift",
    "authors": [
      {
        "name": "Fabrice"
      }
    ],
    "links": {
      "repository": "https://git.schaub-dev.xyz/cppuniverse/sable.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}