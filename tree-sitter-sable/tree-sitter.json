{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sable",
      "camelcase": "Sable",
      "title": "Sable",
      "scope": "source.sable",
      "file-types": [
        "sable"
      ],
      "injection-regex": "^sable$",
      "class-name": "TreeSitterSable",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for the Sable language.",
    "authors": [
      {
        "name": "Codex",
        "email": "codex@openai.com"
      }
    ],
    "links": {
      "repository": "https://example.com/sable"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
